class Bubblesort 
{ 
public static void main(String args[])
 { 
int ar[]={50,40,30,20,10,5}; 
int n=ar.length; int i,j,temp; 
for(i=0;i<n-1;i++)//total number of passes or iterations
 { for(j=0;j<n-1-i;j++)//total number of comparisions {
 if(ar[j]>ar[j+1]) 
{
 temp=ar[j];
 ar[j]=ar[j+1]; ar[j+1]=temp; 
} 
} 
}
 System.out.print("After sorting :");
 for(i=0;i<n;i++) 
System.out.print(ar[i]+" ");
 } 
}
